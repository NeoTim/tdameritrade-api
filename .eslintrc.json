{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "prettier/prettier": "error"
  }
}
